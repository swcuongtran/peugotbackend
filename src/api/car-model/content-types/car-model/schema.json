{
  "kind": "collectionType",
  "collectionName": "car_models",
  "info": {
    "singularName": "car-model",
    "pluralName": "car-models",
    "displayName": "CarModel"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "price": {
      "type": "biginteger"
    },
    "description": {
      "type": "blocks"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "SUV",
        "Sedan",
        "Hatchback"
      ]
    },
    "spec_engine": {
      "type": "string"
    },
    "spec_safety": {
      "type": "string"
    },
    "spec_warranty": {
      "type": "string"
    }
  }
}
